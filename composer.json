{
    "name": "tymon/repository",
    "description": "Repositories",
    "keywords": [
        "tymon",
        "repository",
        "package"
    ],
    "homepage": "https://github.com/tymondesigns/repository",
    "license": "MIT",
    "authors": [
        {
            "name": "Sean Tymon",
            "email": "tymon148@gmail.com",
            "homepage": "http://tymondesigns.com",
            "role": "Developer"
        }
